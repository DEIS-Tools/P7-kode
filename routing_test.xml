	<declaration>// Place global declarations here.

clock total;

const int NUM_STATIONS = 10;
const int MAX_ROBOTS = 5;

typedef int[1,NUM_STATIONS] station_t;

const int dist[station_t][station_t] = {
    {  0,  5,  7,  5,  4,  8,  2,  3,  9, 12 },
    {  2,  0,  9, 12,  1,  2,  6,  3,  8,  4 },
    {  8,  4,  0,  1,  2, 11,  1, 14, 18,  7 },
    { 20,  4, 13,  0,  3, 12, 10,  6,  5, 16 },
    {  2,  6, 17,  3,  0,  1,  7,  9, 16, 12 },
    { 18, 11,  8, 14,  4,  0, 10, 13, 19,  5 },
    {  9, 18, 14,  8,  3,  7,  0, 13,  6,  2 },
    { 17, 10, 19,  5,  1, 12, 15,  0, 11, 20 },
    { 17, 20,  5, 12, 16, 11, 13,  9,  0,  1 },
    {  8, 10, 18,  7,  4,  6,  3, 14,  2,  0 }
};

broadcast chan stoc_arr[station_t], arr[station_t], done;</declaration>
	<template>
		<name>Arrival</name>
		<declaration>
const int prob[station_t] = { 1, 3, 5, 7, 9, 9, 7, 5, 3, 1 };</declaration>
		<location id="id0" x="8" y="0">
			<label kind="exponentialrate" x="34" y="-8">1:5</label>
		</location>
		<branchpoint id="id1" x="-195" y="0">
		</branchpoint>
		<init ref="id0"/>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="select" x="-144" y="68">id : station_t</label>
			<label kind="synchronisation" x="-144" y="85">stoc_arr[id]!</label>
			<label kind="probability" x="-144" y="102">prob[id]</label>
			<nail x="-195" y="59"/>
			<nail x="8" y="59"/>
		</transition>
		<transition controllable="false">
			<source ref="id0"/>
			<target ref="id1"/>
		</transition>
	</template>
	<template>
		<name>Robot</name>
		<declaration>clock x;

bool visited[station_t] = {false, false, false, false, false, false, false, false, false, false};

station_t curr_loc;
station_t dest;

bool allvisited() {
    for (id : station_t) {
        if (!visited[id]) {
            return false;
        }
    }

    return true;
}</declaration>
		<location id="id2" x="-637" y="-246">
			<name x="-714" y="-272">Choose</name>
			<urgent/>
		</location>
		<location id="id3" x="-398" y="-246">
			<name x="-433" y="-289">EnRoute</name>
			<label kind="invariant" x="-467" y="-229">x &lt;= dist[curr_loc][dest]+2</label>
		</location>
		<location id="id4" x="-187" y="-246">
			<name x="-161" y="-255">Arriving</name>
			<urgent/>
		</location>
		<location id="id5" x="-187" y="-51">
			<name x="-170" y="-59">Waiting</name>
		</location>
		<init ref="id2"/>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="-178" y="-178">arr[curr_loc]!</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-500" y="-68">done?</label>
			<label kind="assignment" x="-501" y="-102">x=0,
visited[curr_loc]=true</label>
			<nail x="-637" y="-51"/>
		</transition>
		<transition controllable="false">
			<source ref="id3"/>
			<target ref="id4"/>
			<label kind="guard" x="-340" y="-289">x &gt;= dist[curr_loc][dest]</label>
			<label kind="assignment" x="-340" y="-272">curr_loc = dest</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="select" x="-569" y="-297">id : station_t</label>
			<label kind="guard" x="-569" y="-280">!visited[id]</label>
			<label kind="assignment" x="-569" y="-263">dest = id</label>
		</transition>
	</template>
	<template>
