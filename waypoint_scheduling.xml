bool visited[waypoint_t];

station_t dest;

waypoint_t cur_waypoint = 21;
waypoint_t dest_waypoint;

bool is_stuck(waypoint_t node) {
    bool local_visited[waypoint_t];
    int[0, NUM_WAYPOINTS] stack[waypoint_t];
    int top = 1;
    waypoint_t top_waypoint;

    if (node == station_waypoint[dest]) {
        return false;
    }

    for (id : waypoint_t) {
        local_visited[id] = visited[id];
    }

    stack[top++] = node;

    while (top &gt; 1) {
        top_waypoint = stack[--top];
        stack[top + 1] = 0;

        for (id : waypoint_t) {
            if (!local_visited[id] &amp;&amp; waypoint_dist[top_waypoint][id] != -1) {
                if (id == station_waypoint[dest]) {
                    return false;
                }
                
                local_visited[id] = true;
                stack[top++] = id;
            }
        }
    }

    return true;
}</declaration>
